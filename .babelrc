{
    "presets": ["@babel/preset-react"],
    "plugins": [
        ["@babel/plugin-proposal-decorators", {
            "legacy": true
        }],
        ["@babel/plugin-proposal-class-properties", {
            "loose": true
        }],
        ["import", {
            "libraryName": "antd-mobile",
            "style": "css"
        }] // `style: true` 会加载 less 文件
    ]
}